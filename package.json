{
  "name": "hopmed-mobile",
  "main": "expo/AppEntry.js",
  "version": "1.0.11",
  "scripts": {
    "postinstall": "patch-package",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android:sdk": "node scripts/ensure-local-properties.js",
    "eas:preview:android": "eas build --profile preview --platform android",
    "eas:preview:ios": "eas build --profile preview --platform ios",
    "eas:prod:android": "eas build --profile production --platform android",
    "eas:prod:ios": "eas build --profile production --platform ios",
    "smoke": "node scripts/smoke-check.js",
    "verify:environment": "node scripts/verify-deployment.js",
    "setup:fastlane": "node scripts/setup-fastlane.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "build:verify": "node scripts/verify-build-config.js",
    "build:clean": "expo prebuild --clean",
    "build:preview": "eas build --clear-cache --profile preview --platform android",
    "build:fix-deps": "npm run build:verify && npm run build:clean && npm run build:preview",
    "test": "jest --config jest.config.ci.js",
    "test:watch": "jest --config jest.config.ci.js --watch",
    "env:switch": "node scripts/manage-environments.js switch",
    "env:validate": "node scripts/manage-environments.js validate",
    "env:list": "node scripts/manage-environments.js list",
    "env:setup": "node scripts/manage-environments.js setup",
    "env:status": "node scripts/manage-environments.js status",
    "env:help": "node scripts/manage-environments.js help",
    "env:test": "node scripts/verify-deployment.js",
    "fastlane:setup:ios": "./scripts/setup-fastlane-ios.sh",
    "fastlane:setup:android": "./scripts/setup-fastlane-android.sh",
    "fastlane:setup": "./scripts/setup-fastlane-ios.sh && ./scripts/setup-fastlane-android.sh",
    "fastlane:restore:android": "./scripts/restore-fastlane-android.sh",
    "prebuild:android": "expo prebuild --platform android --clear && npm run fastlane:restore:android",
    "deploy:testflight:qa": "./scripts/deploy-testflight.sh -e development",
    "deploy:testflight:staging": "./scripts/deploy-testflight.sh -e staging",
    "deploy:testflight:production": "./scripts/deploy-testflight.sh -e production",
    "deploy:testflight:quick": "./scripts/deploy-testflight.sh --skip-prebuild",
    "deploy:playstore:qa": "./scripts/deploy-play-store.sh -e development -t internal -b apk",
    "deploy:playstore:staging": "./scripts/deploy-play-store.sh -e staging -t alpha -b aab",
    "deploy:playstore:production": "./scripts/deploy-play-store.sh -e production -t beta -b aab",
    "deploy:playstore:release": "./scripts/deploy-play-store.sh -e production -t production -b aab",
    "deploy:playstore:quick": "./scripts/deploy-play-store.sh --skip-prebuild -t alpha -b aab",
    "deploy:playstore:full": "npm run prebuild:android && npm run deploy:playstore:staging",
    "deploy:playstore:patch": "./scripts/deploy-play-store.sh -e staging -t alpha --version-type patch",
    "deploy:playstore:minor": "./scripts/deploy-play-store.sh -e staging -t alpha --version-type minor",
    "deploy:playstore:major": "./scripts/deploy-play-store.sh -e staging -t alpha --version-type major",
    "deploy:playstore:force": "./scripts/deploy-play-store.sh -e staging -t alpha --force --skip-validation",
    "deploy:playstore:no-git": "./scripts/deploy-play-store.sh -e staging -t alpha --no-auto-commit --no-auto-tag",
    "ios:health": "cd ios && bundle exec fastlane health_check",
    "ios:clean": "cd ios && bundle exec fastlane clean",
    "ios:info": "cd ios && bundle exec fastlane build_info",
    "android:health": "./scripts/restore-fastlane-android.sh > /dev/null 2>&1; cd android && bundle exec fastlane health_check",
    "android:clean": "./scripts/restore-fastlane-android.sh > /dev/null 2>&1; cd android && bundle exec fastlane clean",
    "android:info": "./scripts/restore-fastlane-android.sh > /dev/null 2>&1; cd android && bundle exec fastlane build_info",
    "build:playstore:aab": "./scripts/build-play-store-manual.sh -t aab",
    "build:playstore:apk": "./scripts/build-play-store-manual.sh -t apk",
    "build:playstore:staging": "./scripts/build-play-store-manual.sh -t aab -e staging",
    "build:playstore:production": "./scripts/build-play-store-manual.sh -t aab -e production",
    "build:test:apk": "./scripts/build-eas-simulation.sh",
    "build:test:debug": "./build-debug-apk.sh",
    "build:test:simple": "./build-apk-simple.sh",
    "install:notifee": "./scripts/install-notifee.sh",
    "verify:notifee": "node scripts/verify-notifee-setup.js",
    "rebuild:notifee": "npx expo prebuild --clean && npm run android"
  },
  "dependencies": {
    "@daily-co/config-plugin-rn-daily-js": "^0.0.9",
    "@daily-co/react-native-daily-js": "0.80.0",
    "@daily-co/react-native-webrtc": "^118.0.3-daily.4",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^3.3.2",
    "@kingstinct/react-native-healthkit": "^11.0.0",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^21.14.0",
    "@react-native-firebase/messaging": "^21.14.0",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.0",
    "@sentry/react-native": "~6.14.0",
    "@supersami/rn-foreground-service": "^2.2.5",
    "@types/node": "^24.3.0",
    "@types/react": "~19.0.10",
    "axios": "^1.6.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "expo": "~53.0.23",
    "expo-av": "~15.1.7",
    "expo-background-fetch": "~13.1.6",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.8",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.0",
    "expo-local-authentication": "~16.0.5",
    "expo-location": "~18.1.6",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.4",
    "expo-screen-orientation": "^8.1.7",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-task-manager": "~13.1.6",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-hook-form": "^7.48.2",
    "react-native": "0.79.5",
    "react-native-background-timer": "^2.4.1",
    "react-native-callkeep": "^4.3.16",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.10.0",
    "react-native-health-connect": "^3.4.0",
    "react-native-nitro-modules": "^0.29.8",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-voip-push-notification": "^3.3.3",
    "react-redux": "^9.0.0",
    "redux-persist": "^6.0.0",
    "rxjs": "^7.8.2",
    "yup": "^1.3.3",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@config-plugins/react-native-callkeep": "11.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.42.0",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "jest-junit": "^16.0.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.0",
    "sharp": "^0.33.4",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-encrypted-storage",
          "redux-persist",
          "rxjs",
          "@kingstinct/react-native-healthkit",
          "react-native-health-connect",
          "react-native-nitro-modules",
          "@daily-co/react-native-daily-js",
          "@daily-co/config-plugin-rn-daily-js",
          "@daily-co/react-native-webrtc",
          "react-native-background-timer",
          "@notifee/react-native",
          "@react-native-firebase/app",
          "@react-native-firebase/messaging",
          "react-native-voip-push-notification"
        ]
      }
    }
  },
  "overrides": {
    "@expo/config-plugins": "~10.1.1",
    "@react-native-async-storage/async-storage": "2.1.2"
  },
  "resolutions": {
    "@react-native-async-storage/async-storage": "2.1.2"
  },
  "private": true
}
